{
  "name": "dentistry-backend",
  "version": "2.0.0",
  "main": "server.js",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-babel": "babel ./src -d ./build/src && babel server.js -d ./build && babel router.js -d ./build",
    "build": "yarn run build-babel && yarn env",
    "env": "cp .env ./build/.env",
    "production": "npm run build && node ./build/server.js",
    "dev": "nodemon --ignore ./src/middleware --exec babel-node server.js",
    "start": "node ./build/server.js",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "app": "yarn build && yarn env && yarn start"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "env": "^0.0.2",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mongoose": "^6.9.1",
    "mongoose-paginate-v2": "^1.7.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/runtime": "^7.20.13",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4"
  }
}